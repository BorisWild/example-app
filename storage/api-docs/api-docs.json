{
    "openapi": "3.0.0",
    "info": {
        "title": "API Documentation",
        "description": "Documentation API",
        "contact": {
            "email": "admin@admin.com"
        },
        "license": {
            "name": "Apache 2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
        },
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "/api",
            "description": "API Server"
        },
        {
            "url": "/",
            "description": "Session Server"
        }
    ],
    "paths": {
        "/chatgpt/get-message-by-prompt": {
            "get": {
                "tags": [
                    "ChatGPT"
                ],
                "summary": "Get answer from ChatGPT without context",
                "operationId": "get_message_by_prompt",
                "parameters": [
                    {
                        "name": "prompt",
                        "in": "query",
                        "description": "Message to ChatGPT",
                        "schema": {
                            "type": "string",
                            "example": "Hello"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "answer": {
                                            "type": "string",
                                            "example": "Hello!"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "$ref": "#/components/responses/404"
                    },
                    "403": {
                        "$ref": "#/components/responses/403"
                    },
                    "422": {
                        "$ref": "#/components/responses/422"
                    },
                    "401": {
                        "$ref": "#/components/responses/401"
                    }
                }
            }
        },
        "/pdf/generate-from-html": {
            "post": {
                "tags": [
                    "PDF"
                ],
                "summary": "Generate pdf from html. Return a pdf stream",
                "operationId": "generate_from_html",
                "parameters": [
                    {
                        "name": "html",
                        "in": "query",
                        "description": "HTML to generate",
                        "schema": {
                            "type": "string",
                            "example": "<h1>Hello<h1>"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success"
                    },
                    "404": {
                        "$ref": "#/components/responses/404"
                    },
                    "403": {
                        "$ref": "#/components/responses/403"
                    },
                    "422": {
                        "$ref": "#/components/responses/422"
                    },
                    "401": {
                        "$ref": "#/components/responses/401"
                    }
                }
            }
        }
    },
    "components": {
        "responses": {
            "404": {
                "description": "Not Found"
            },
            "403": {
                "description": "Don't have permissions",
                "content": {
                    "application/json": {
                        "schema": {
                            "properties": {
                                "message": {
                                    "type": "string",
                                    "example": "You don't have permissions to process this request"
                                }
                            },
                            "type": "object"
                        }
                    }
                }
            },
            "401": {
                "description": "Unauthenticated",
                "content": {
                    "application/json": {
                        "schema": {
                            "properties": {
                                "message": {
                                    "type": "string",
                                    "example": "Unauthenticated."
                                }
                            },
                            "type": "object"
                        }
                    }
                }
            },
            "422": {
                "description": "Error validation",
                "content": {
                    "application/json": {
                        "schema": {
                            "properties": {
                                "message": {
                                    "type": "string",
                                    "example": "The given data was invalid."
                                },
                                "errors": {
                                    "properties": {
                                        "email": {
                                            "type": "array",
                                            "items": {
                                                "example": "Must be a valid email."
                                            }
                                        },
                                        "password": {
                                            "type": "array",
                                            "items": {
                                                "example": "More than 8 chars."
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            },
                            "type": "object"
                        }
                    }
                }
            }
        }
    },
    "tags": [
        {
            "name": "ChatGPT",
            "description": "ChatGPT"
        },
        {
            "name": "PDF",
            "description": "PDF"
        }
    ]
}